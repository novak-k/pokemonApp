{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "target": "es6",
    "strict": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "./build",
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "jsx": "react-jsx"
  },
  "include": ["vite.config.ts"]
}
